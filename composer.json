{
    "name": "mikimaine/ecommerce",
    "description": "The Laravel Ecommerce Framework.",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Mikiyas Amdu",
            "email": "mikias.amdu@gmail.com",
            "homepage": "https://github.com/mikimaine/ecommerce",
            "role": "Developer"
        }
    ],
  "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "hesto/multi-auth": "^1.0",
        "laravelcollective/html": "dev-master",
        "infyomlabs/laravel-generator": "5.3.x-dev",
        "infyomlabs/adminlte-templates": "5.3.x-dev",
        "infyomlabs/generator-builder": "dev-master",
        "doctrine/dbal": "2.6.x-dev",
        "arcanedev/log-viewer": "^3.9.0",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "cviebrock/eloquent-sluggable": "^3.1",

        "spatie/laravel-tail": "^1.1",
        "orangehill/iseed": "^2.1",
        "yajra/laravel-datatables-oracle": "^6.0",
        "filp/whoops": "^2.0",
        "spatie/activitylog": "^2.4",
        "spatie/laravel-backup": "^3.2",
        "gloudemans/shoppingcart": "^1.3",
        "barryvdh/laravel-ide-helper": "^2.2",
    "pagerfanta/pagerfanta": "^1.0",
    "lukepolo/laracart": "^1.1",
    "michelf/php-markdown": "^1.6",
    "maatwebsite/laravel-sidebar": "^2.1",
    "baum/baum": "^1.1",
        "caffeinated/themes": "^3.0"
    },
    "require-dev": {
        "symfony/dom-crawler": "~3.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "phpspec/phpspec": "~2.1",
        "barryvdh/laravel-debugbar": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Innovate\\": "app/Innovate",
            "Modules\\": "Modules"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "scripts": {
        "post-install-cmd": [
            
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
          "php artisan clear-compiled",
          "php artisan ide-helper:generate",
          "php artisan ide-helper:meta",
          "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "repo-name": {
            "type": "git",
            "url": "git@github.com:jamisonvalenta/Laravel-4-Generators.git"
        }
    }
}
